    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "nodemon": "^2.0.15",
    "rimraf": "^5.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.4"
  }
}
