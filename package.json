{
  "name": "finance-api",
  "version": "1.0.1",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc --sourceMap false --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "bun dist/app.js",
    "start:dev": "env-cmd -f .env ts-node-dev --exit-child -r tsconfig-paths/register ./src/app.ts",
    "typecheck": "bun run tsc --noEmit",
    "db:seed": "bun run prisma db seed",
    "db:setup": "./scripts/setup-database.sh",
    "db:reset": "bun run prisma db push --force-reset && bun run db:seed",
    "postinstall": "bun run prisma generate --no-engine",
    "postinstall:postgres": "bun run prisma generate",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.js,.json",
    "lint:fix": "eslint . --fix --ext .ts,.js,.json"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/neximo/finance-api.git#readme",
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@auth/prisma-adapter": "^2.11.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-sqs": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@cliengo/logger": "^1.1.1",
    "@fast-csv/format": "^5.0.5",
    "axios": "^1.12.2",
    "bignumber.js": "^9.0.2",
    "bluebird": "^3.7.2",
    "bson-objectid": "^2.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-jwt": "^8.4.1",
    "ioredis": "^5.0.4",
    "jose": "^5.10.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.7.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.11",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "0.11.3",
    "routing-controllers-openapi": "^5.0.0",
    "short-unique-id": "^5.3.2",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.6.3",
    "ts-pattern": "^5.8.0",
    "typedi": "0.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {

    "prisma": "^6.17.1",
    "@types/bluebird": "^3.5.37",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.192",
    "@types/luxon": "^3.7.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
